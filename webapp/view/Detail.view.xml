<mvc:View controllerName="prueba.preexamen.controller.Detail"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page title="{i18n>detail-title}" id="detail">
        <content>

            <ObjectHeader
		title="{ContactName}"
		number="{
			parts:[{path:'Price'},{path:'CurrencyCode'}],
			type: 'sap.ui.model.type.Currency',
			formatOptions: {showMeasure: false}
		}"
		numberUnit=""
		class="sapUiResponsivePadding--header" >
            <statuses>
                <ObjectStatus
                    text="Some Damaged"
                    state="Error" />
                <ObjectStatus
                    text="In Stock"
                    state="Success" />
            </statuses>
            <ObjectAttribute text="Phone: {Phone}" />
            <ObjectAttribute text="{ContactTitle}" />
            <ObjectAttribute text="{Address}, {City}" />
            <ObjectAttribute
                text="www.sap.com"
                active="true"
                press="handleLinkObjectAttributePress" />
	</ObjectHeader>

        </content>
    </Page>
</mvc:View>
